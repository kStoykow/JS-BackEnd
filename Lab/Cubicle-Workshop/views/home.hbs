<header>
    <ul>
        <li><img class="logo" src="/static/images/logo.png"></li>
        <li><a href="/">Browse</a></li>
        <li><a href="/about">About</a></li>
        {{#if user}}
        <li><a href="/create/cube">Add a Cube</a></li>
        <li><a href="/create/accessory">Add Accessory</a></li>
        <li><a href="/logout">Logout</a></li>
        {{else}}
        <li><a href="/login">Login</a></li>
        <li><a href="/register">Register</a></li>
        {{/if}}
    </ul>
</header>
<h1>Browser</h1>
<form action="/" method="/get">
    <input type="text" class="search" name="search" placeholder="search" value="{{search}}">
    <input type="number" name="from" class="difficulty" placeholder="Difficulty from..." value="{{from}}">
    <span>-</span>
    <input type="number" name="to" class="difficulty" placeholder="Difficulty to..." value="{{to}}">
    <input type="submit" value="search">
</form>
{{#if cubes}}
{{#each cubes}}
{{> cubeOptions}}
{{/each}}
{{else}}
<h1>NO CUBES WERE FOUND</h1>
{{/if}}